// Object{} -> variable: data 변수에 데이터값 부여 /*콤마 꼭 붙이기*/
const mjInfo = {
  name: "mj",
  age: 22,
  born: "Busan",
  favSingers: ["Arashi", "KinKiKids", "TaylorSwift", "HarryStyles"],
  favTVshow: [
    {
      name: "BingBangtheory",
      fun: true
    },
    {
      name: "Friends",
      fun: true
    }
  ]
}

console.log(mjInfo.age); // 22
mjInfo.age = 23;
console.log(mjInfo.age); // 23
console.log(favSingers); // [~]
console.log(mjInfo.favTVshow[0]); // {name: "BingBangtheory", fun: true}
console.log(mjInfo.favTVshow[0], name); // BigBangtheory
